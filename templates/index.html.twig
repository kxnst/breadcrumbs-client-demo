{% extends 'base.html.twig' %}
{% block body %}
    {% for breadcrumb in breadcrumbs %}
        {{ breadcrumb | raw }}
    {% endfor %}
{% endblock %}
{% block stylesheets %}
<style>
    .breadcrumb-a{
        padding: 5px;
        background-color: blue;
        color: white;
    }
    .breadcrumb-span{
        padding: 5px;
        background-color: black;
        color: white;
    }
    .breadcrumb-a.active{
        background-color: green;
    }
</style>
{% endblock %}